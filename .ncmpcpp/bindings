
def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "ctrl_u"
  page_up

def_key "ctrl_d"
  page_down

def_key "g"
  move_home

def_key "G"
  move_end

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "l"
  require_screen "browser"
  press_enter

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "h"
  require_screen "browser"
  jump_to_parent_directory

def_key "."
  show_lyrics

def_key "n"
  next_found_item

def_key "K"
  move_selected_items_up

def_key "J"
  move_selected_items_down

def_key "D"
  clear_playlist

def_key "D"
  clear_main_playlist

def_key "d"
  delete_playlist_items

def_key "d"
  delete_stored_playlist
